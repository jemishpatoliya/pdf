# Server
PORT=4000
NODE_ENV=development

# Mongo
MONGO_URI=mongodb+srv://<user>:<pass>@<cluster>/<db>?retryWrites=true&w=majority

# Auth
JWT_SECRET=change_me

# Optional: seed admin user (only if you want auto-create on boot)
SEED_ADMIN=false
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=change_me

# Redis / BullMQ
# Set REDIS_DISABLED=true to run without queues
REDIS_DISABLED=true
REDIS_URL=rediss://:<password>@<host>:<port>
REDIS_MODE=
WORKER_DEBUG=false

# AWS S3
AWS_REGION=ap-south-1
AWS_S3_BUCKET=your-bucket-name
# If running on AWS with instance roles, you can omit AWS_ACCESS_KEY_ID/AWS_SECRET_ACCESS_KEY.
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Print agent download (optional)
PRINT_AGENT_S3_KEY=
PRINT_AGENT_FILENAME=SecurePrintHub-Setup.exe
